@model DAL.UserMaster

@{
    ViewBag.Title = "User Profile";
}



<div class="col-xl-12">
    <div class="game_show" id="create-user">
        <div class="row justify-content-center">
            <div class="col-xl-6 col-md-7 col-lg-7">
               <div class="login_form">

                <div class="form-group">
                    <label>Name:</label>

                    @Html.DisplayFor(model => model.FirstName, new { htmlAttributes = new { @class = "form-control" } })


                </div>

                <div class="form-group">
                    <label>Email Adderess:</label>

                    @Html.DisplayFor(model => model.EmailID, new { htmlAttributes = new { @class = "form-control" } })


                </div>
                <div class="form-group">
                    <label>Contact Number:</label>

                    @Html.DisplayFor(model => model.LastName, new { htmlAttributes = new { @class = "form-control" } })


                </div>

                <div class="submit-box mt-2">
                    <a class="btn submit-btn"  href="@Url.Action("EditProfile", "Home")">Edit</a>
                </div>
                   </div>
                


            </div>
        </div>
    </div>
</div>
