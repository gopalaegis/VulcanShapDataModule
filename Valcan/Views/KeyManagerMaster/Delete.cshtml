@model Valcan.Models.KeyManagerViewModel


@{
    ViewBag.Title = "Delete";
}






<div class="col-xl-12 g-xl-0 g-lg-0">
    <div class="game_show" id="change-password">
        <div class="row justify-content-center">
            <div class="col-xl-6 col-md-7 col-lg-7">
                <div class="login_form">
                    <h5>Are you sure you want to delete this?</h5>
                    <div>

                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.KeyManager)
                            </dt>
                            <dt class="h4">
                                @Html.DisplayFor(model => model.KeyManager)
                            </dt>

                            @*<dd>
                @Html.DisplayNameFor(model => model.IsActive)
            </dd>
            <dt>
                @Html.DisplayFor(model => model.IsActive)
            </dt>*@



                        </dl>
                        <div class="form-actions no-color">

                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()
                                @Html.DisplayFor(model => model.msg)
                                <br />
                                if (Model != null && Model.msg == "")
                                {
                                    <input type="submit" value="Delete" class="btn btn-danger" />
                                }

                                <button type="button" onclick="history.back()" class="btn btn-primary text-white">
                                    Back
                                </button>

                            }
                        </div>
                        </div>
                    </div>
            </div>
        </div>
    </div>
</div>


